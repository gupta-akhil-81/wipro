
resources:
- name: {{ env['name'] }}
  type: storage.v1.bucket
  properties:
    location: {{ properties['region'] }}
    storageClass: {{ properties['storageClass'] }}
    labels:
       environment: {{ properties['environment'] }}
       product: {{ properties['product'] }}
    versioning:
      enabled: false
    lifecycle:
      rule:
      - action:
          type: Delete
        condition:
          age: {{ properties['age'] }}
  